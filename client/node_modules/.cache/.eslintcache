e.error('Failed to retrieve journal entries:', error.message);\r\n      }\r\n    };\r\n\r\n    fetchJournalEntries();\r\n  }, [email]);\r\n\r\n  const currentDate = new Date();\r\n\r\n  return (\r\n    <ThemeProvider theme={createTheme()}>\r\n      <CssBaseline />\r\n\r\n      <main>\r\n        {/* Hero unit */}\r\n        <Box\r\n          sx={{\r\n            bgcolor: 'paper',\r\n            pt: 8,\r\n            pb: 6,\r\n          }}\r\n        >\r\n          <Container maxWidth=\"sm\">\r\n            <Typography\r\n              component=\"h1\"\r\n              variant=\"h2\"\r\n              align=\"center\"\r\n              color=\"text.primary\"\r\n              gutterBottom\r\n            >\r\n              Hey,\r\n            </Typography>\r\n            <Typography variant=\"h5\" align=\"center\" color=\"text.secondary\" paragraph>\r\n              your entries are below.\r\n            </Typography>\r\n            <Stack\r\n              sx={{ pt: 4 }}\r\n              direction=\"row\"\r\n              spacing={2}\r\n              justifyContent=\"center\"\r\n            >\r\n            <Button\r\n              variant=\"contained\"\r\n              onClick={handleCreateClick}\r\n              sx={{\r\n                bgcolor: 'lightgrey',\r\n                color: 'black',\r\n                '&:hover': {\r\n                  bgcolor: 'black',\r\n                  color: 'white',\r\n                },\r\n              }}\r\n            >\r\n              +\r\n            </Button>\r\n              \r\n            </Stack>\r\n          </Container>\r\n        </Box>\r\n        <Container sx={{ py: 8 }} maxWidth=\"md\">\r\n          {/* End hero unit */}\r\n          <Grid container spacing={4}>\r\n            {journalEntries.map((entry) => (\r\n              <Grid item key={entry.id} xs={12} sm={6} md={4}>\r\n                <Card\r\n                  sx={{\r\n                    height: '100%',\r\n                    display: 'flex',\r\n              